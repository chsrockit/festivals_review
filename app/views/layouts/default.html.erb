<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= controller.action_name %></title>
    <link href='http://fonts.googleapis.com/css?family=Amaranth' rel='stylesheet' type='text/css'>
    <%= stylesheet_link_tag 'reset', '960', 'application' %>
    <%= javascript_include_tag 'https://ajax.googleapis.com/ajax/libs/jquery/1.6.0/jquery.min.js'%>
    <%= javascript_include_tag 'https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.12/jquery-ui.min.js'%>
    <%= javascript_include_tag 'application' %>
  </head>
  <body class="festivals">
    <div class="page container_12">
      <div class="page_header grid_12">
        <h1>Festivals Review</h1>
      </div>
      <div class="clear"></div>
      <div id="page_content" class="grid_12">
        <%= yield %>
      </div>
      <div id="page_footer" class="grid_12">
        <p class="credits">Created for <a href="http://culturehackscotland.com">Culture Hack Scotland 2011</a></p>
      </div>
    </div>
  </body>
</html>
