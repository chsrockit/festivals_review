<h1>Listing events</h1>

<table>

<% @events.each do |event| %>
  <tr>
    <td><%=h event.name %></td>
    <td><%=h event.festival_id %></td>
    <td><%=h event.show_id %></td>
    <td><%=h event.venue_id %></td>
    <td><%=h event.score %></td>
    <td><%= link_to 'Show', event %></td>
  </tr>
<% end %>
</table>

<br />

